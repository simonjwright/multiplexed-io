aggregate project SPI extends "../environment.gpr" is

   case RTS is
      when "adaracer" =>
         for Project_Path use ("../stm32f429x");
      when "stm32f4" =>
         for Project_Path use ("../stm32f40x");
   end case;

   for Project_Files use ("spi_build.gpr");

end SPI;
